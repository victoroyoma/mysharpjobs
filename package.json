{
  "name": "magic-patterns-vite-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "build:analyze": "cross-env ANALYZE=true npx vite build",
    "build:production": "cross-env NODE_ENV=production npx vite build",
    "start:performance": "cross-env REACT_APP_PERFORMANCE_MONITORING=true npx vite",
    "analyze:bundle": "npx webpack-bundle-analyzer dist/assets/*.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "preview": "npx vite preview",
    "test:performance": "cross-env NODE_ENV=test npm run build && npm run analyze:bundle"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "laravel-echo": "^2.2.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.441.0",
    "pusher-js": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "latest",
    "cross-env": "^10.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "latest",
    "tailwindcss": "3.4.17",
    "typescript": "^5.5.4",
    "vite": "^5.2.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
